
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'

config interface 'lan'
	option type 'bridge'
	option proto 'static'
	option netmask '255.255.255.0'
	option ipaddr '192.168.2.1'
	option ip6assign '64'
	option _orig_ifname 'eth0.1 wlan0 wlan1'
	option _orig_bridge 'true'
	option ifname 'eth0.4'

config device 'lan_dev'
	option name 'eth0.1'
	option macaddr '1c:40:e8:11:df:3d'

config interface 'wan'
	option _orig_ifname 'eth0.2'
	option _orig_bridge 'false'
	option proto 'pppoe'
	option username '13527881117@139.gd'
	option password '881117'
	option ipv6 'auto'
	option keepalive '0'
	option ifname 'eth1'

config device 'wan_dev'
	option name 'eth0.2'
	option macaddr '1c:40:e8:11:df:3c'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '3'
	option vid '3'
	option ports '0t 4'

config interface 'IPTV'
	option proto 'dhcp'
	option ifname 'eth0.3'
	option delegate '0'
	option defaultroute '0'
	option macaddr 'F0:9C:D7:EC:6B:93'

config switch_vlan
	option device 'switch0'
	option vlan '4'
	option vid '4'
	option ports '0t 1 2'

config switch_vlan
	option device 'switch0'
	option vlan '5'
	option vid '5'
	option ports '0t 3'

config interface 'TR069'
	option proto 'dhcp'
	option ifname 'eth0.5'
	option defaultroute '0'

